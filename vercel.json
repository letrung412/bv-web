{
  "version": 2,
  "env": {
  },
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["**"] }
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static",
      "config": { "includeFiles": ["**"] }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "dist/index.js"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "build/$1"
    }
  ]
}