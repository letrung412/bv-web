{
  "version": 2,
  "env": {
  },
  "builds": [
    {
      "src": "backend/dist/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["**"] }
    },
    {
      "src": "backend/build/index.html",
      "use": "@vercel/static",
      "config": { "includeFiles": ["**"] }
    }
  ],
  "routes": [
    {"handle": "filesystem"},
    {
      "src": "/.*",
      "dest": "backend/dist/index.js"
    },
    {
      "src": "/",
      "dest": "backend/build/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "backend/build/$1"
    }
  ]
}